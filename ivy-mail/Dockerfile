FROM axonivy/axonivy-engine:nightly-10.0

RUN mkdir /usr/lib/axonivy-engine/deploy/myApp
RUN wget -O /usr/lib/axonivy-engine/deploy/myApp/myApp.zip -q -N https://dev.axonivy.com/maven/com.axonivy.demo/ivy-demos-app/10.0/zip
RUN wget -O /usr/lib/axonivy-engine/deploy/myApp/custom-mail.zip -q -N https://dev.axonivy.com/maven/com.axonivy.demo.custom.mail/custom-mail-demo/10.0/iar
